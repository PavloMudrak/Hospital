@model CustomIdentityApp.ViewModels.CreateDoctorViewModel
@{
    ViewBag.Title = "Adding doctor";
}
<form asp-action="Create1" method="post">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="Name" class="control-label">Name</label>
        <input type="text" asp-for="Name" class="form-control" />
    </div>
    @*<div class="form-group">
        <label asp-for="DoctorId" class="control-label">DoctorId</label>
        <input type="text" asp-for="DoctorId" class="form-control" />
    </div>*@
    <div class="form-group">
        <label asp-for="Year" class="control-label">Year of birdth</label>
        <input type="number" asp-for="Year" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Proffesion" class="control-label">Proffesion</label>
        <input type="text" asp-for="Proffesion" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="ADD" class="btn btn-default" />
    </div>
</form>