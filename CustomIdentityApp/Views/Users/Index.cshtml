@model IEnumerable<CustomIdentityApp.Models.User>
@{
    ViewBag.Title = "Users list ";
}

<a asp-action="Create" class="btn btn-sm btn-primary">Add user</a>

<table class="table">
    <tr><th>Login</th><th>Year of birdth</th><th></th></tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.NormalizedUserName</td>
            <td>@user.Year</td>
            <td>
                <form asp-action="Delete" asp-route-id="@user.Id" method="post">
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-id="@user.Id">Change</a>
                    <a class="btn btn-sm btn-primary" asp-action="ChangePassword" asp-route-id="@user.Id">Change password</a>

                    <button type="submit" class="btn btn-sm btn-danger">
                        Delete user
                    </button>
                </form>
            </td>
        </tr>
    }
</table>